<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="site_actions" meta_type="CMF Action Category" purge="true">
  <object name="accessibility" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Accessibility</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/acessibilidade</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>  
  <object name="contraste" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Alto Contraste</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:#</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="mapadosite" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Site Map</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${globals_view/navigationRootUrl}/mapadosite</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="plone_setup" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Site Setup</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${portal_url}/@@overview-controlpanel</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Plone Site Setup: Overview"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 <object name="user" meta_type="CMF Action Category">
  <object name="login" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
  </object>
 </object>
</object>
