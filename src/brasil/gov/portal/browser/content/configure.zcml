<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="brasil.gov.portal">

  <!-- Overrides do Doormat-->
  <browser:page
     for="Products.Doormat.content.interfaces.IDoormat"
     layer="brasil.gov.portal.interfaces.IBrasilGov"
     name="doormat-view"
     class=".doormat.DoormatView"
     template="templates/doormat_view.pt"     
     permission="zope2.View"
     />  
  <browser:page
     for="Products.Doormat.content.interfaces.IDoormat"
     layer="brasil.gov.portal.interfaces.IBrasilGov"
     name="doormat-view-bare"
     class=".doormat.DoormatView"
     template="templates/doormat_bare_view.pt"     
     permission="zope2.View"
     />

  <browser:resource
     name="search.js"
     file="js/search.js"
     layer="brasil.gov.portal.interfaces.IBrasilGov"
     />

  <browser:page
     name="busca"
     class="plone.app.search.browser.Search"
     permission="zope2.View"
     for="*"
     template="templates/search.pt"
     />

  <browser:page
      for="*"
      name="mapadosite"
      class="Products.CMFPlone.browser.sitemap.SitemapView"
      template="templates/sitemap.pt"
      permission="zope.Public"
      allowed_interface="brasil.gov.portal.interfaces.IBrasilGov"
      />

  <browser:viewlet
      name="plone.htmlhead.title"
      manager="plone.app.layout.viewlets.interfaces.IHtmlHead"
      class="plone.app.layout.viewlets.common.TitleViewlet"
      template="templates/title.pt"
      permission="zope2.View"
      layer="brasil.gov.portal.interfaces.IBrasilGov"
      />

</configure>
