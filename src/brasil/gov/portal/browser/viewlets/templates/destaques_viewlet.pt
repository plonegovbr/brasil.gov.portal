<div id="portal-features" tal:condition="viewlet/available">
    <a href="" tal:define="portal_url viewlet/portal_url"
        tal:condition="viewlet/editable"
        tal:attributes="href string:${portal_url}/destaques">Editar destaques</a>
    <script async="async">
    jq(function() {
        jq('#featured-content').load(portal_url + '/destaques?ajax_load=1 #content > .row');
    });
    </script>
    <div id="featured-content"></div>
</div>
